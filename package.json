{
  "name": "eeg-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "backend:install": "cd backend && pip install -r requirements.txt",
    "backend:migrate": "cd backend && python manage.py migrate",
    "backend:dev": "cd backend && python manage.py runserver 8000",
    "backend:admin": "cd backend && python manage.py createsuperuser",
    "dev:full": "concurrently \"npm run dev\" \"npm run backend:dev\"",
    "setup": "npm install && npm run backend:install && npm run backend:migrate"
  },
  "dependencies": {
    "next": "^15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "20.19.12",
    "@types/react": "19.1.12",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "5.9.2"
  }
}
